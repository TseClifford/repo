<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
  <addon id="repository.tseclifford" version="1.0.0" name="TseClifford Repository" provider-name="tseclifford">
  <extension point="xbmc.addon.repository" name="TseClifford Repository">
    <dir>
      <info compressed="false">https://raw.githubusercontent.com/TseClifford/repository.tseclifford/master/repo/zips/addons.xml</info>
      <checksum>https://raw.githubusercontent.com/TseClifford/repository.tseclifford/master/repo/zips/addons.xml.md5</checksum>
      <datadir zip="true">https://raw.githubusercontent.com/TseClifford/repository.tseclifford/master/repo/zips/</datadir>
    </dir>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary>Clifford's Kodi Repository</summary>
    <description>A collection of plugins and repositories"</description>
    <platform>all</platform>
		<assets>
			<icon>icon.png</icon>
		</assets>
  </extension>
</addon>
  <addon id="plugin.video.icdrama" version="2.3.4" name="Icdrama" provider-name="aznhusband, dumbo0001, s_h, boleanly, mugol, General-Meow, cthlo, soraxas, andya4">
    <requires>
      <import addon="script.module.html5lib" version="0.999.0"/>
      <import addon="script.module.beautifulsoup4" version="4.3.2"/>
      <import addon="script.module.requests" version="2.9.1"/>
      <import addon="script.module.resolveurl" version="5.1.86"/>
      <import addon="script.module.future" version="0.17.1" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="addon.py">
      <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
      <summary>Dramas, shows, and movies from icdrama.se</summary>
      <description>Provides contents from icdrama.se. "Happy wife, happy life"</description>
      <disclaimer>This addon is not affiliated with icdrama.se</disclaimer>
      <platform>all</platform>
      <license>MIT License</license>
      <assets>
        <icon>icon.png</icon>
      </assets>
    </extension>
  </addon>
</addons>